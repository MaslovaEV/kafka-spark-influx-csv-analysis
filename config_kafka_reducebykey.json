{
  "input": {
    "input_type": "kafka",
    "options": {
      "server": "localhost",
      "port": 29092,
      "topic": "demo1",
      "batchDuration": 2,
      "sep": ","
    }
  },
  "output": {
    "method": "stdout",
    "options": {
      "influx": {
        "host":"176.120.25.195",
        "port": 8086,
        "username": "root",
        "password": "root",
        "database": "test_reducebykey",
        "measurement": "points"
      }
    }
  },
  "processing": {
    "transformation": "src_country: country(src_ip);packet_size;traffic: mult(packet_size,sampling_rate)",
    "aggregations": {
      "operation_type": "reduceByKey",
      "rule": "key = (src_country, src_mac), Max(packet_size), Sum(traffic)"
    }
  },
  "databases": {
    "country": "./GeoLite2-Country.mmdb",
    "city": "./GeoLite2-City.mmdb",
    "asn": "./GeoLite2-ASN.mmdb"
  }
}