{
  "input": {
    "input_type": "csv",
    "options": {
    "filename": "test/data/test.csv"
    }
  },
  "output": {
    "method": "influx",
    "options": {
      "influx": {
        "host": "localhost",
        "port": 8086,
        "username": "root",
        "password": "root",
        "database": "test",
        "measurement": "points"
      }
    }
  },
  "processing": {
    "transformation": "source_ip: src_ip; dst_ip;src_country: country_of(src_ip);traffic: mult(packet_size,sampling_rate)",
    "aggregations": {
			"operation_type": "reduce",
 			"rule": "Min(traffic), Sum(traffic)"
		}
  }
}