{
  "input": {
    "input_type": "csv",
    "options": {
    "filename": "test/data/test.csv"
    }
  },
  "output": {
    "method": "csv",
    "options": {
      "csv": {
        "path": "1.csv",
        "encoding": "utf-8",
        "sep": ";"
      }
    }
  },
  "processing": {
    "transformation": "src_ip;packet_size;traffic: mult(packet_size,sampling_rate)",
    "aggregations": {
			"operation_type": "reduceByKey",
 			"rule": "key = src_ip; Sum(traffic); Sum(packet_size)"
		}
  }
}